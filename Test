local function RIWO_fake_script()
	local script = Instance.new('LocalScript', EmoteButton)

	script.Parent.Draggable = true
	local OpenSound = Instance.new("Sound", script)
	OpenSound.SoundId = "rbxassetid://14863866"
	OpenSound.Name = "Click"
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.EmoteBG.Visible = not script.Parent.Parent.EmoteBG.Visible
		OpenSound:Play()
	end)
end
coroutine.wrap(RIWO_fake_script)()
local function FKPD_fake_script()
	local script = Instance.new('LocalScript', EmoteWheelUI)

	local gui = script.Parent
	local uis = game:GetService("UserInputService")
	local OpenSound = script.Parent.EmoteButton.LocalScript:WaitForChild("Click")
	
	uis.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.B then
			script.Parent.EmoteBG.Visible = not script.Parent.EmoteBG.Visible
			OpenSound:Play()
		end	
	end)
end
coroutine.wrap(FKPD_fake_script)()

game:GetService("Players").LocalPlayer.Character.Humanoid.Died:Connect(function()
	coroutine.wrap(loadstring(game:HttpGet("https://github.com/danya2854/Myscripts/raw/refs/heads/main/Test")))()
	game.Players.LocalPlayer:WaitForChild("PlayerGui").EmoteWheelUI:Destroy()
end)
